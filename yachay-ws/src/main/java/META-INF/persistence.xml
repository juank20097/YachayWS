<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	 <persistence-unit name="yachaywsPU">
    	<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <jta-data-source>java:/yachaywsDatasource</jta-data-source>
        <class>yachayws.model.entidades.GenCatalogoCab</class>
        <class>yachayws.model.entidades.GenCatalogoItemsDet</class>
        <class>yachayws.model.entidades.GenExterno</class>
        <class>yachayws.model.entidades.GenParametro</class>
        <class>yachayws.model.entidades.GenPersona</class>
        <class>yachayws.model.entidades.GenPersonaDetalle</class>
        <class>yachayws.model.entidades.GenSalud</class>
        <class>yachayws.model.entidades.GenWsMail</class>        
        <properties>
            <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQLDialect" />
            <property name="hibernate.hbm2ddl.auto" value="validate"/>
            <property name="hibernate.show_sql" value="false" />
            <property name="hibernate.format_sql" value="false" />
        </properties>
    </persistence-unit>
</persistence>
